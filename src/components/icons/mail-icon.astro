---
const { classNamed } = Astro.props;

import style from '../../styles/icon.module.css'
---

<svg
  aria-hidden="true"
  viewBox="0 0 16 16"
  height="16"
  width="16"
  xmlns="http://www.w3.org/2000/svg"
  fill-rule="evenodd"
  clip-rule="evenodd"
  stroke-linejoin="round"
  stroke-miterlimit="1.414"
  class={`${classNamed} ${style.icon}`}
>
  <path
    d="M0 4l8 5 8-5V3c0-1.1-.9-2-2-2H2C.9 1 0 1.9 0 3v1zm0 2v7c0 1.1.9 2 2 2h12c1.1 0 2-.9 2-2V6l-8 5-8-5z"
    fill-rule="nonzero"
  />
</svg>